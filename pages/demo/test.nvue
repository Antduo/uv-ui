<template>
	<view>
		<uv-code-input v-model="value" :disabledKeyboard="true" @change="change" @finish="finish"></uv-code-input>
	</view>
</template>
<script>
	export default {
		data(){
			return {
				value: 12
			}
		},
		onLoad() {
			setTimeout(()=>{
				this.value = '123456'
			},1000)
		},
		methods: {
			change(e) {
				console.log('内容改变，当前值为：' + e);
			},
			finish(e) {
				console.log('输入结束，当前值为：' + e);
			}
		}
	}
</script>